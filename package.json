{
  "name": "@examplary/developer-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build-api-docs": "docusaurus gen-api-docs all && rm docs/rest-api/examplary-api.info.mdx",
    "build": "docusaurus clean-api-docs all && yarn build-api-docs && docusaurus build",
    "deploy": "serverless deploy --stage production"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.2",
    "@docusaurus/preset-classic": "3.9.2",
    "@docusaurus/theme-mermaid": "^3.9.2",
    "@mdx-js/react": "^3.1.1",
    "docusaurus-plugin-generate-llms-txt": "^0.0.1",
    "docusaurus-plugin-openapi-docs": "^4.5.1",
    "docusaurus-theme-openapi-docs": "^4.5.1",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@docusaurus/types": "3.9.2",
    "serverless": "^4.22.0",
    "serverless-lift": "^1.32.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  }
}
